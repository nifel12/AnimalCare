<div class="login">
<h2> REGISTRAR DUEÑO </h2>
<form method="post"  class="form">
   
        <input type="hidden" asp-for = "dueno.Id" class="inputs"/>
        <label asp-for="dueno.Nombres" class="label"></label>
        <input asp-for="dueno.Nombres" class="inputs">
        <label asp-for="dueno.Apellidos" class="label"></label>
        <input asp-for="dueno.Apellidos" class="inputs">
        <label asp-for="dueno.Direccion" class="label"></label>
        <input asp-for="dueno.Direccion" class="inputs">
        <label asp-for="dueno.Telefono" class="label"></label>
        <input asp-for="dueno.Telefono" class="inputs">
        <label asp-for="dueno.Correo" class="label"></label>
        <input asp-for="dueno.Correo" class="inputs">


        <button type="submit" class="primary-button login-button">Guardar</button>
    

</form>
